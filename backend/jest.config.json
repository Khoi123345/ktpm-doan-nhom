{
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
        "src/**/*.js",
        "!src/server.js",
        "!src/config/**",
        "!src/routes/**",
        "!src/models/**",
        "!src/utils/seedAdmin.js**",
        "!src/app.js**"
    ],
    "testMatch": [
        "**/tests/**/*.test.js"
    ],
    "transform": {},
    "moduleNameMapper": {
        "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testPathIgnorePatterns": [
        "/node_modules/"
    ]
}